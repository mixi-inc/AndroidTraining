<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.mixi.sample.intent"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="jp.mixi.sample.intent.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="jp.mixi.sample.intent.NewActivity"
            android:label="@string/app_name"/>
        <activity
            android:name="jp.mixi.sample.intent.ResultActivity"
            android:label="@string/app_name"/>
        <activity
            android:name="jp.mixi.sample.intent.ExtraActivity"
            android:label="@string/app_name"/>
    </application>

</manifest>